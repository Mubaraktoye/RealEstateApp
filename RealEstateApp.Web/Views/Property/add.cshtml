@{
    ViewData["Title"] = "Property Page";
    Layout = "~/Views/Shared/_FrontPageLayout.cshtml";
    //var returnUrl = @Context.Request.Query["returnUrl"];
}

<!-- Add Property Start -->
<div class="container-xxl py-5">
    <div class="container">

        <div class="row g-4">
            <div class="col-md-12">
                <div class="wow fadeInUp" data-wow-delay="0.5s">
                    <p class="mb-4">Please fill this form to add a property.</p>
                    <form>
                        <div class="row g-3">
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <select class="form-control" name="propertyaction" id="property-action">
                                        <option autofocus>Choose Action</option>
                                        <option value="1">For Rent</option>
                                        <option value="2">For Sell</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="property-title" placeholder="Property Title">
                                    <label for="name">Title</label>
                                </div>
                            </div>
                            
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="property-location" placeholder="Property Location">
                                    <label for="name">Property Location</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="property-landmark" placeholder="Landmark">
                                    <label for="name">Landmark</label>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-floating">
                                    <input type="number" class="form-control" id="property-rooms" placeholder="No of Rooms">
                                    <label for="email">No of Rooms</label>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-floating">
                                    <input type="number" class="form-control" id="property-price" placeholder="Price">
                                    <label for="email">Price</label>
                                </div>
                            </div>
                            
                            <div class="col-md-6">
                                
                                <div class="form-floating">
                                    <input type="file" class="form-control" id="property-pictures" accept=".png, .jpg, .jpeg" multiple>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-floating">
                                    <textarea class="form-control" placeholder="Other Description" id="property-description" style="height: 150px"></textarea>
                                    <label for="message">Description</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <label for="isgated">Click here if the property is fenced/gated</label>
                                <div class="form-floating">
                                    <input name="isgated" type="checkbox" id="property-is-gated" placeholder="Is Gated">
                                </div>
                            </div>

                            <div class="col-12">
                                <div class="btn btn-primary w-100 py-3" onclick="OnSubmitPropertyRequest()" type="submit">Add Property</div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Add Property End -->

