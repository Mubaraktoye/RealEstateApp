@section Scripts{
    <script type="text/javascript">

        // function RunRecallFormValidation(){

        // }

        function OnSubmitPropertyRequest() {

            var propertyType = $("#property-action").val();
            var propertyGated = $("#property-is-gated").prop("checked");
            var propertyTitle = $("#property-title").val();
            var propertyDescription = $("#property-description").val();
            var propertyRooms = $("#property-rooms").val();
            var propertyPrice = $("#property-price").val();
            var propertyLocation = $("#property-location").val();
            var propertyLandmark = $("#property-landmark").val();
            var propertyPics = document.getElementById('property-pictures').files;

            if (propertyType == 0 || propertyTitle == null || propertyDescription == null || propertyPrice == 0 || propertyLocation == null || propertyLandmark == null) {
                toastr.error("a required filed is missing");
            }
            else{
                var formData = new FormData();
                formData.append("propertyType", propertyType);
                formData.append("propertyGated", propertyGated);
                formData.append("propertyTitle", propertyTitle);
                formData.append("propertyDescription", propertyDescription);
                formData.append("propertyRooms", propertyRooms);
                formData.append("propertyPrice", propertyPrice);
                formData.append("propertyLocation", propertyLocation);
                formData.append("propertyLandmark", propertyLandmark);
                for (var i = 0; i != propertyPics.length; i++) {
                    formData.append("propertyPics", propertyPics[i]);
                    //formData.append("files", files[i]);
                }
                
                SubmitPropertyForm(formData);
            }
        }

        function SubmitPropertyForm(data) {
            $.ajax({
                url: '@Url.Action("AddProperty", "Property")',
                type: 'POST',
                enctype: 'multipart/form-data',
                processData: false,
                contentType: false,
                dataType: 'json',
                data: data,
                success: function (response) {
                    if (response.status) {
                        toastr.success(response?.message);
                        //location.reload();
                    }
                    else {
                        toastr.warning(response?.message);
                    }
                    return;
                }, error: function () {
                    //alerty("danger", "Opps", "request Failed");
                    console.log("Request Failed");
                }
            });
        }


    </script>
}